/* miniprogram/pages/toolbox/seatManager/seatManager.wxss */
.orderBar{
  position: fixed;
  z-index: 1000;
  width: 100%;
}

movable-view {
  background: rgb(221, 229, 244)
}

movable-area {
  height: 90vh;
  width: 100%;
  background-color: #ccc;
  overflow: hidden;
}

.grid_cell {
	width:30px;
	height:30px;
	display:block;
	line-height:normal;
	overflow:hidden;
	position:absolute;
	transition:all ease 0.05s;
	-webkit-transition:all ease 0.05s;
}

.grid_1{
  width:28px;
	height:28px;
	display:block;
	overflow:hidden;
	position:absolute;
	font-size:8px;
	text-align:center;
}

.grid_1 text {
	background-image:url('data:image/png;base64,');
	background-repeat:no-repeat;
	background-size:100% 100%;
	display:block;
	width:20px;
	height:20px;
  line-height: 20px;
	font-style:normal;
	color:#353535;
}

